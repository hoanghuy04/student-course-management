{
  "name": "student-course-management-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "antd": "^5.10.3",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "json-server": "^0.17.4",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "json-server --watch db.json --port 8080 --delay 500",
    "start:all": "concurrently --kill-others --names \"API,APP\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run start:server\" \"npm start\"",
    "dev": "npm run start:all",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

